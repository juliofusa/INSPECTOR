<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.eulen.inspector.CALIBRADO_ADM">

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="date"
        android:ems="10"
        android:id="@+id/ADM_FECHA"
        android:hint="FECHA"
        android:layout_below="@+id/textView86"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_toLeftOf="@+id/textView91"
        android:layout_toStartOf="@+id/textView91"
        android:gravity="center_horizontal"
        android:editable="false"
        android:singleLine="true"
        android:clickable="false"
        android:enabled="false"
        android:textColor="#fff11f00" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="time"
        android:ems="10"
        android:id="@+id/ADM_HORA"
        android:layout_below="@+id/textView87"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:hint="HORA"
        android:enabled="false"
        android:layout_toLeftOf="@+id/textView91"
        android:layout_toStartOf="@+id/textView91"
        android:gravity="center_horizontal"
        android:editable="false"
        android:clickable="false"
        android:singleLine="true"

        android:textColor="#fff11f00" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="VERIFICAR ALARMAS ALAMAN"
        android:id="@+id/ADM_TITULO"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="FECHA"
        android:id="@+id/textView86"
        android:layout_below="@+id/ADM_TITULO"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="HORA"

        android:id="@+id/textView87"
        android:layout_below="@+id/ADM_FECHA"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="18dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="FILTRO"
        android:id="@+id/textView90"
        android:layout_below="@+id/ADM_HORA"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="18dp" />

    <Spinner
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ADM_SPIN_FILTRO"
        android:layout_below="@+id/textView90"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="#ffe2e5ff"
        android:layout_alignRight="@+id/textView91"
        android:layout_alignEnd="@+id/textView91" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="OBSERVACIONES"
        android:id="@+id/textView88"
        android:layout_below="@+id/ADM_SPIN_FILTRO"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:visibility="invisible"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ADM_OBSERVACIONES"
        android:layout_below="@+id/textView88"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/ADM_SPIN_FILTRO"
        android:layout_alignEnd="@+id/ADM_SPIN_FILTRO"
        android:singleLine="true"
        android:visibility="invisible" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="ANOTADO POR:"
        android:id="@+id/textView89"
        android:layout_below="@+id/ADM_OBSERVACIONES"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Spinner
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ADM_SPIN_PERSONA"
        android:layout_below="@+id/textView89"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/ADM_SPIN_FILTRO"
        android:layout_alignEnd="@+id/ADM_SPIN_FILTRO"
        android:background="#ffffe0df" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GUARDAR"
        android:id="@+id/ADM_GUARDAR"
        android:drawableLeft="@android:drawable/ic_menu_save"
        android:drawablePadding="8dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/ADM_HORA"
        android:layout_alignEnd="@+id/ADM_HORA"
        android:onClick="GUARDAR"
        android:gravity="center"
        android:background="@drawable/boton_redondo_gris"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Medium              Text"
        android:id="@+id/textView91"
        android:layout_alignBottom="@+id/ADM_TITULO"
        android:layout_toRightOf="@+id/ADM_TITULO"
        android:layout_marginLeft="65dp"
        android:layout_marginStart="65dp"
        android:visibility="invisible" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MAQUINA DE TRAZAS"
        android:id="@+id/ADM_TRAZAS_CHECK"
        android:checked="false"
        android:layout_below="@+id/ADM_SPIN_PERSONA"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="21dp"
        android:onClick="maquina_TRAZAS"
        android:visibility="invisible" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="REVISION MAQUINA DE TRAZAS"
        android:id="@+id/TEXT_REVISION"
        android:layout_below="@+id/ADM_TRAZAS_CHECK"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="18dp"
        android:visibility="invisible" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ADM_REVISON_TRAZAS"
        android:layout_below="@+id/TEXT_REVISION"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/textView91"
        android:layout_alignEnd="@+id/textView91"
        android:singleLine="true"
        android:visibility="invisible" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ADM_PANEL"
        android:background="#ffffff15"
        android:textColor="#ff000000"
        android:layout_alignBottom="@+id/ADM_GUARDAR"
        android:layout_alignLeft="@+id/list_CALIBRADO"
        android:layout_alignStart="@+id/list_CALIBRADO"
        android:layout_alignTop="@+id/ADM_GUARDAR"
        android:layout_alignRight="@+id/textView93"
        android:layout_alignEnd="@+id/textView93"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:gravity="center" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView"
        android:src="@drawable/ver_alrm"
        android:layout_alignLeft="@+id/textView91"
        android:layout_alignStart="@+id/textView91"
        android:layout_toStartOf="@+id/list_CALIBRADO"
        android:layout_toLeftOf="@+id/list_CALIBRADO"
        android:layout_alignParentTop="true"
        android:scaleType="fitXY"
        android:layout_alignBottom="@+id/adm_CALENDAR" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/adm_CALENDAR"
        android:src="@drawable/rel_oj"
        android:onClick="HORA_PICK"
        android:visibility="visible"
        android:layout_above="@+id/textView90"
        android:layout_toRightOf="@+id/ADM_HORA"
        android:layout_toStartOf="@+id/imageView"
        android:scaleType="fitXY"
        android:layout_alignTop="@+id/textView87"
        android:background="@drawable/boton_redondo" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/list_CALIBRADO"
        android:layout_below="@+id/textView93"
        android:layout_above="@+id/ADM_PANEL"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:smoothScrollbar="true"
        android:clickable="false"
        android:focusable="false"
        android:descendantFocusability="blocksDescendants"
        android:layout_toRightOf="@+id/ADM_SPIN_FILTRO"
        android:layout_toEndOf="@+id/ADM_SPIN_FILTRO"
        android:layout_marginLeft="5dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="VERIFICACIONES REALIZADAS:                                    "
        android:id="@+id/textView93"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/list_CALIBRADO"
        android:layout_alignStart="@+id/list_CALIBRADO" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="00"
        android:id="@+id/l_IDENTIFICADOR"
        android:layout_above="@+id/ADM_SPIN_FILTRO"
        android:layout_toLeftOf="@+id/list_CALIBRADO"
        android:layout_toStartOf="@+id/list_CALIBRADO"
        android:textColor="@android:color/white"
        android:background="@android:color/black"
        android:visibility="invisible" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/FECHA_SELECCION"
        android:layout_below="@+id/list_CALIBRADO"
        android:layout_toRightOf="@+id/ADM_PANEL"
        android:layout_toLeftOf="@+id/imageButton10"
        android:layout_toStartOf="@+id/imageButton10"
        android:gravity="center_horizontal"
        android:enabled="false"
        android:layout_alignParentBottom="true"
        android:textColor="@android:color/holo_green_dark" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton10"
        android:layout_alignBottom="@+id/FECHA_SELECCION"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/list_CALIBRADO"
        android:src="@android:drawable/ic_menu_today"
        android:onClick="FECHA_PICK"
        android:background="@drawable/boton_redondo_gris" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="REALIZAR EL CALIBRADO AL COMPLETO."
        android:gravity="center_horizontal"
        android:id="@+id/ADM_ATENCION"
        android:layout_toRightOf="@+id/textView93"
        android:layout_alignTop="@+id/textView93"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:textColor="#ffff1e00"
        android:visibility="invisible" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ACTUALIZAR"
        android:id="@+id/ADM_ACTUALIZA"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/adm_CALENDAR"
        android:layout_toLeftOf="@+id/list_CALIBRADO"
        android:layout_toStartOf="@+id/list_CALIBRADO"
        android:background="#ff9fff75"
        android:onClick="ACTUALIZA_BOTON"
        android:visibility="invisible" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ALARMA VERIFICADA"
        android:id="@+id/ALARMA_ALAMAN"
        android:background="@android:color/holo_red_light"
        android:layout_below="@+id/ADM_REVISON_TRAZAS"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="37dp"
        android:layout_toLeftOf="@+id/textView91"
        android:layout_toStartOf="@+id/textView91"
        android:visibility="invisible"
        android:onClick="ALARMA" />

</RelativeLayout>
